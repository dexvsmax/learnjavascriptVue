    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.2",
    "dropzone": "^5.5.1",
    "flatpickr": "^4.6.3",
    "vee-validate": "^3.0.11",
